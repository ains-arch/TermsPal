{
  "manifest_version": 3,
  "name": "Privacy Policy Tracker",
  "version": "1.0",
  "description": "Tracks the number of privacy policies accepted while browsing.",
  "permissions": ["storage"],
  "background": {
      "scripts": ["background.js"]
  },
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
